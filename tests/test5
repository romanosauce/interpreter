program {
    struct test2 {
        string str = "eee";
        int ii = 3;
        int i = 2;
        boolean flag = true;
    };
    struct test {
        string str = "abcd";
        int i = 2;
        boolean flag = false;
    };
    test t1, t2;
    test2 t3;
    boolean flag;
    read(flag);
    write(flag);
    read(t2.flag);
    write(t2.flag);
    write(t2.str);
}
