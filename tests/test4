program { /* Проверка на метки */
    int a = 123, i = 90;
    if(a == 123) {
        read(a);
        goto F;
    }
    else {}
R:  write("R mark!\n");
    goto END;

F:  write("F mark!\n");
    for(;;) {
        write("yes");
        i = i + 1;
        write(i);
        if(i == 100) {
            goto M;
        } else {}
    }
M:  write("M mark!\n", "i = ", i, "\n");
    goto R;
END:write("END mark!\n");
}
